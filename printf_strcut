#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>

typedef struct stu//结构体类型
{
	//成员变量；
	char name[20];
	short age;
	char number[12];
	char sex[5];
	char id[10];
}stu;



int main()
{
	stu s1 = {"zhangsan",20,"123456","男","2020212121"};//s-结构体变量；
	stu s2 = { "zhang",16,"12345678","女","2020313131" };
	printf("name: %s\n", s1.name);
	//类似以下：
	/*int arr1[] = { 1,2,3,4,56,7,8 };
	int arr2[] = { 2,35,7,8,34,24,5 };*/
	return 0;
}
